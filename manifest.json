{
  "manifest_version": 2,

  "name": "NeoCache",
  "description": "Caching of the future TM",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": [],
      "js": [
        "lib/jquery/jquery-1.11.2.min.js",
        "lib/parse/parse-1.3.4.min.js",
        "payloader.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}